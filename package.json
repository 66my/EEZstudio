{
    "name": "eezstudio",
    "author": "Envox <eez@envox.hr>",
    "description": "Cross-platform visual development tool and SCPI instrument controller",
    "homepage": "https://www.envox.hr/eez/studio/studio-introduction.html",
    "version": "0.9.84",
    "revision": "1",
    "license": "GPL-3.0-only",
    "repository": "https://github.com/eez-open/studio",
    "main": "build/main/main.js",
    "scripts": {
        "postinstall": "electron-builder install-app-deps",
        "start": "electron .",
        "build": "npm run build-src && npm run build-css && npm run make-electron-builder-yml",
        "build-src": "tsc && node node_modules/gulp-cli/bin/gulp.js release",
        "build-css": "node node_modules/less/bin/lessc --clean-css packages/eez-studio-ui/_stylesheets/main.less build/eez-studio-ui/_stylesheets/main.css",
        "make-electron-builder-yml": "cd installation && tsc && cd .. && node installation/make-electron-builder-yml.js",
        "watch-all": "concurrently \"npm run watch\" \"npm run watch-less\"",
        "watch": "node node_modules/gulp-cli/bin/gulp.js debug && tsc --project tsconfig.dev.json",
        "watch-less": "node watch-less.js",
        "tslint": "tslint --project ./tsconfig.json 'packages/**/*.{ts,tsx}'",
        "tsc-version": "tsc --version",
        "electron-version": "electron --version",
        "publish-types": "npm publish packages/eez-studio-types",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "trailingComma": "none",
        "arrowParens": "avoid"
    },
